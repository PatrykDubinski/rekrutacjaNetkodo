<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <link
      rel="stylesheet"
      href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css"
      integrity="sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z"
      crossorigin="anonymous"
    />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Rekrutacja</title>
  </head>
  <body>
    <main role="main">
      <section class="jumbotron text-center">
        <div class="container">
          <h1 class="jumbotron-heading">Photos from file</h1>
        </div>
      </section>

      <div class="album py-5 bg-light">
        <div class="container">
            <% data.imagesArray.forEach(result => { %>
            <div class="col-md-12">
              <div class="card mb-4 box-shadow d-flex justify-content-center align-items-center">
                <div class="card-body">
                  <img class="img-fluid" src="<%= result %>" />
                </div>
              </div>
            </div>
            <% }); %>

    </main>
  </body>
</html>
